{
    "school template": {
        "prefix": "stemp",
        "body": [
            "\\documentclass{article}",
            "",
            "\\begin{document}",
            "",
            "$0",
            "",
            "\\end{document}"
        ],
        "description": "school template"
    },
    "align": {
        "prefix": "ali",
        "body": [
            "\\begin{align*}",
            "\t$1",
            "\\end{align*}",
            "$0"
        ],
        "description": "align environment"
    },
    "environment": {
        "prefix": "beg",
        "body": [
            "\\begin{$1}",
            "\t$2",
            "\\end{$1}",
            "$0"
        ]
    },
    "math mode": {
        "prefix": "m",
        "body": [
            "\\($1\\)$0"
        ],
        "description": "math mode"
    },
    "figure": {
        "prefix": "fig",
        "body": [
            "\\begin{figure}[!ht]",
            "\t\\centering",
            "\t\\includegraphics{$1}",
            "\t\\caption{$2}",
            "\\end{figure}",
            "$0"
        ],
        "description": "figure environment"
    },
    "theorem": {
        "prefix": "th",
        "body": [
            "\\begin{theorem}[$1]",
            "$2",
            "\\end{theorem}",
            "$0"
        ],
        "description": "theorem environment"
    },
    "itemize": {
        "prefix": "item",
        "body": [
            "\\begin{itemize}",
            "\t\\item $1",
            "\\end{itemize}",
            "$0"
        ],
        "description": "itemize environment"
    },
    "equation": {
        "prefix": "eq",
        "body": [
            "\\begin{equation*}",
            "\t$1",
            "\\end{equation*}",
            "$0"
        ],
        "description": "equation environment"
    },
    "tabular": {
        "prefix": "tab",
        "body": [
            "\\begin{center}",
            "\\begin{tabular}{$1}",
            "\t$2",
            "\\end{tabular}",
            "\\end{center}",
            "$0"
        ],
        "description": "tabular environment"
    },
    "paragraph": {
        "prefix": "para",
        "body": [
            "\\paragraph{$1}$0"
        ],
        "description": "paragraph"
    },
    "implies": {
        "prefix": "imp",
        "body": [
            "\\implies $0"
        ],
        "description": "implies"
    },
    "bold face": {
        "prefix": "bf",
        "body": [
            "\\textbf{$1}$0"
        ],
        "description": "bold face"
    },
    "italicize": {
        "prefix": "it",
        "body": [
            "\\textit{$1}$0"
        ],
        "description": "italicize"
    },
    "text": {
        "prefix": "t",
        "body": [
            "\\text{$1}$0"
        ],
        "description": "text"
    },
    "proof": {
        "prefix": "pr",
        "body": [
            "\\begin{proof}",
            "$1",
            "\\end{proof}",
            "$0"
        ],
        "description": "proof environment"
    },
    "definition": {
        "prefix": "def",
        "body": [
            "\\begin{definition}[$1]",
            "$2",
            "\\end{definition}",
            "$0"
        ],
        "description": "definition environment"
    },
    "display equation": {
        "prefix": "e",
        "body": [
            "\\[",
            "$1",
            "\\]",
            "$0"
        ],
        "description": "display equation"
    },
    "mono text": {
        "prefix": "tt",
        "body": [
            "\\texttt{$1}$0"
        ],
        "description": "mono text"
    },
    "right arrow": {
        "prefix": "->",
        "body": [
            "\\rightarrow$0"
        ],
        "description": "right arrow"
    },
    "left arrow": {
        "prefix": "<-",
        "body": [
            "\\leftarrow$0"
        ],
        "description": "left arrow"
    },
    "times": {
        "prefix": "x",
        "body": [
            "\\times$0"
        ],
        "description": "times"
    },
    "enum": {
        "prefix": "enum",
        "body": [
            "\\begin{enumerate}",
            "\t\\item $1",
            "\\end{enumerate}",
            "$0"
        ],
        "description": "enumerate environment"
    },
    "sum": {
        "prefix": "sum",
        "body": [            
            "\\sum_{$1}^{$2} $0"
        ],
        "description": "summation"
    },
    "product": {
        "prefix": "prod",
        "body": [
            "\\prod_{$1}^{$2} $0"
        ],
        "description": "product"
    },
    "<=>": {
        "prefix": "<=>",
        "body": [
            "\\Leftrightarrow"
        ],
        "description": "<=>"
    },
    "fraction": {
        "prefix": "frac",
        "body": [
            "\\frac{$1}{$2}$0"
        ],
        "description": "fraction"
    },
    "textrm": {
        "prefix": "rm",
        "body": [
            "\\textrm{$1}$0"
        ],
        "description": "textrm"
    },
    "example": {
        "prefix": "ex",
        "body": [
            "\\begin{exmp}[$1]",
            "$2",
            "\\end{exmp}",
            "$0"
        ],
        "description": "example"
    },
    "parens": {
        "prefix": "()",
        "body": [
            "\\left($1\\right)"
        ],
        "description": "big-parens"
    },
    "squirlyBrackets": {
        "prefix": "{}",
        "body": [
            "\\{$1\\\\}$0"
        ],
        "description": "squirlyBrackets"
    },
    "rightArrow": {
        "prefix": "=>",
        "body": [
            "\\Rightarrow$0"
        ],
        "description": "rightArrow"
    }
}
