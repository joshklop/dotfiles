[user]
	name = Josh Klopfenstein
	email = joshklop10@gmail.com
[alias]
	cgraph = log --all --decorate --oneline --graph
	st = status -s
[pull]
	rebase = false
[merge]
	tool = nvim
	conflictstyle = diff3
	prompt = false
[init]
	defaultBranch = master
[credential "https://github.com"]
	helper =
	helper = !/usr/bin/gh auth git-credential
